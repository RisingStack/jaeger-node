    "nyc": "11.1.0",
    "pg": "6.4.0",
    "request": "2.81.0",
    "request-promise-native": "1.0.4",
    "restify": "5.2.0",
    "sinon": "3.2.1",
    "sinon-chai": "2.13.0",
    "super-request": "1.2.0"
